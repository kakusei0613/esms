<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.esms.mapper.IEducationMapper">
    <select id="selectAll" resultType="com.example.esms.domain.Education">
        SELECT * FROM education;
    </select>
    <insert id="insert" parameterType="com.example.esms.domain.Education">
        INSERT INTO education(name) VALUES(#{name});
    </insert>
    <delete id="deleteById" parameterType="java.lang.Integer">
        DELETE FROM education WHERE id = #{id};
    </delete>
</mapper>